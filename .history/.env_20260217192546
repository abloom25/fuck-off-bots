ENVIRONMENT=dev
DRIVER=~fastapi+~websockets
LOCALSTORE_USE_CWD=true

# OneBot V11 Adapter Configuration

# Mode 1: Reverse WebSocket (Recommended)
# NoneBot acts as a server. NapCat connects to NoneBot.
# Configure NapCat to connect to: ws://127.0.0.1:8080/onebot/v11/ws
PORT=8080
HOST=127.0.0.1

# Mode 2: Forward WebSocket (Client)
# NoneBot connects to NapCat.
# If you use this, uncomment the line below and set the correct NapCat WebSocket URL.
# ONEBOT_WS_URLS=["ws://127.0.0.1:3001"]

# Access Token (if set in NapCat)
# ONEBOT_ACCESS_TOKEN=your_token_here

# Superusers (List of QQ numbers that can manage the bot list)
SUPERUSERS=["123456789"]  # Replace with your QQ number

# Command Prefix
COMMAND_START=["/", ""]
